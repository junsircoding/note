   "source": [
    "import requests\n",
    "url = \"https://www.baidu.com/img/bd_logo1.png\"\n",
    "response = requests.get(url)\n",
    "with open(\"baidu.png\", \"wb\") as f:\n",
    "    f.write(response.content)\n"
