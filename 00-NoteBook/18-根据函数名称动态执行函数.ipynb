    "# getattr函数\n",
    "class M:\n",
    "    def __init__(self):\n",
    "        pass\n",
    "\n",
    "    def exam_func(self):\n",
    "        print(\"running\")\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    m = M()\n",
    "    getattr(m, \"exam_func\")()\n",
    "\n",
    "# out:\n",
    "# running\n"
