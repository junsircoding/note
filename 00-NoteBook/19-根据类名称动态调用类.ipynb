    "# eval函数\n",
    "class MyCls:\n",
    "    def __init__(self, p1):\n",
    "        self.p1 = p1\n",
    "\n",
    "    def show(self):\n",
    "        print(self.p1)\n",
    "\n",
    "\n",
    "cls_name = \"MyCls\"\n",
    "NewCls = eval(cls_name)\n",
    "new_cls = NewCls(p1=\"test_p1\")\n",
    "new_cls.show()\n"
   ]
